{
  "name": "reactive-entity",
  "version": "1.0.0",
  "description": "A reactive entity model for enterprise data forms",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:playground": "jest -i __tests__/Playground --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Katastrophe831/ReactiveEntity.git"
  },
  "keywords": [
    "reactive entity",
    "typescript reactive",
    "active record",
    "reactive model",
    "angular reactive forms"
  ],
  "author": "Chon Neth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Katastrophe831/ReactiveEntity/issues"
  },
  "homepage": "https://github.com/Katastrophe831/ReactiveEntity#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",    
    "jest": "^29.4.2",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/validatorjs": "^3.15.0",
    "validatorjs": "^3.22.1"
  }
}
